-t JsonDef @ '{
        "a": 123,
        "b": "Hello, World!",
        "c": true
    }'

-s data

-t Repeat 3

-t JdbcDml '{
        [#if url??] "url": "[=url]", [/#if]
        [#if yieldGeneratedKeys??] "yieldGeneratedKeys": "[=yieldGeneratedKeys]", [/#if]
        "ddl": [
            "CREATE TABLE data (id INTEGER PRIMARY KEY [=autoIncrementKeyword], a INTEGER, b TEXT, c BOOLEAN)"
        ],
        "dml": [
            {
                "statement": "INSERT INTO data (a, b, c) VALUES (?, ?, ?)",
                "parameters": [
                    "data.a",
                    "data.b",
                    "data.c"
                ]
            }
        ]
    }'