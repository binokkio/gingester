{
  "transformers": [
    {
      "transformer": "JsonCreate",
      "parameters": {
        "a": 123,
        "b": "Hello, World!",
        "c": true
      }
    },
    "Stash",
    {
      "transformer": "JdbcDml",
      "parameters": {
        "url": "jdbc:sqlite:test.sqlite3",
        "ddl": "CREATE TABLE test (a INTEGER, b TEXT, c BOOLEAN)",
        "dml": {
          "statement": "INSERT INTO test VALUES (?, ?, ?)",
          "parameters": [
            "stash.a",
            "stash.b",
            "stash.c"
          ]
        }
      }
    }
  ]
}