[#assign tableName="test"]

-t JsonDef '{
        "a": 123,
        "b": "Hello, World!",
        "c": true
    }'
-s
-t JdbcDml '{
        "url": "[=url]",
        "ddl": {
            "template": "/create-table.sql",
            "is": "RESOURCE",
            "kwargs": {
                "tableName": "[=tableName]"
            }
        },
        "dml": {
            "statement": "INSERT INTO [=tableName] VALUES (?, ?, ?)",
            "parameters": [
                "stash.a",
                "stash.b",
                "stash.c"
            ]
        }
    }'